<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">

	<Appenders>
		<Console name="console" target="SYSTEM_OUT">
			<PatternLayout pattern="%d{HH:mm:ss} %t %-5p %c{1}:%L %m%n" />
		</Console>
	</Appenders>

	<Loggers>
		<Root>
			<AppenderRef ref="console" />
		</Root>
	</Loggers>

</Configuration>